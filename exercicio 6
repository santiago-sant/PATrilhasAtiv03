internal class Program
{
    static void Main(string[] args)
    {
        Console.Write("Digite o primeiro número: ");
        int x = int.Parse(Console.ReadLine());

       
        if (x % 2 == 0)
        {
            Console.WriteLine($" Seu numero é par.");
        }
        else
        {
            Console.WriteLine($"O seu numero é impar.");
        }
    }
}
